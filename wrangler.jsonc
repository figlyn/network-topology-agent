{
  "name": "network-topology-agent",
  "compatibility_date": "2026-02-18",
  "main": "src/worker.ts",
  "routes": [{ "pattern": "nwgrm.org/*", "zone_name": "nwgrm.org" }],
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  },
  "env": {
    "staging": {
      "name": "network-topology-agent-staging",
      "routes": [{ "pattern": "staging.nwgrm.org/*", "zone_name": "nwgrm.org" }]
    }
  }
}
