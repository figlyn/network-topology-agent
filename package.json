{
  "name": "network-topology-agent",
  "version": "1.0.0",
  "description": "AI-powered network topology diagram generator for B2B telecom proposals. Converts solution descriptions into Cisco-style topology diagrams with operator cloud model, drag-to-edit, and multi-LLM support.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "deploy": "vite build && npx wrangler deploy",
    "deploy:staging": "vite build && npx wrangler deploy --env staging"
  },
  "dependencies": {
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@resvg/resvg-wasm": "^2.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "network-topology",
    "telecom",
    "cisco",
    "diagram",
    "ai",
    "llm",
    "sd-wan",
    "b2b",
    "proposal"
  ],
  "license": "MIT"
}
